package mb.exception;

public class LoginException extends BWException{

	public enum TYPE{ NIE_ZALOGOWANO };
	protected TYPE type;
	
	
	public LoginException(TYPE t) {
		super();
		this.type=t;
	}


	/**
	 * @return the type
	 */
	public TYPE getType() {
		return type;
	}


	
	
}
